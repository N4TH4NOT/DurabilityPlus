{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "${description}",
  "authors": [
    "ddeeddii",
    "N4TH4NOT"
  ],
  "contact": {
    "homepage": "",
    "sources": "https://github.com/N4TH4NOT/DurabilityPlus",
    "issues": "https://github.com/N4TH4NOT/DurabilityPlus/issues"
  },
  "license": "MIT",
  "icon": "assets/${id}/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "${group_id}.DurabilityPlusClient"
    ]
  },
  "mixins": [
    "${id}.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric": "*",
    "minecraft": ">=${minecraft_version}"
